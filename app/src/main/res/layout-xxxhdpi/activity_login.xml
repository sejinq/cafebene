<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:customviews="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <cosmantic.cosmantic_khw.IngredientChartView
        android:id="@+id/donutChart"
        android:layout_width="93dp"
        android:layout_height="93dp"
        customviews:positive="1"
        customviews:negative="1"
        customviews:type_text="지성"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"/>
    <ImageButton
        android:id="@+id/login_email_join_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/facebook_button"
        android:layout_centerInParent="true"/>
    <ImageButton
        android:id="@+id/login_kakao_sign_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/kakaotalk_icon"
        android:layout_below="@id/login_email_join_btn"
        android:layout_centerHorizontal="true"/>
    <ImageButton
        android:id="@+id/login_facebook_sign_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/facebook_button"
        android:layout_below="@id/login_kakao_sign_btn"
        android:layout_centerHorizontal="true"/>
    <ImageButton
        android:id="@+id/login_email_sign_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/facebook_button"
        android:layout_below="@id/login_facebook_sign_btn"
        android:layout_centerHorizontal="true"/>
</RelativeLayout>